        checkout scm
    }
    stage('Build image') {
        sh "echo 'Build Phase'"
    }
    stage('Deploy image') {
        sh "echo 'Build Phase'"
    }
    stage('Test image') {
        sh "echo 'Tests Phase'"
    }
    stage('Post result') {
        sh "echo 'Post Result Phase'"
    }
}
